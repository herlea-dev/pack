{
    "name": "herlea/pack",
    "description": "A tiny package generator for Laravel",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Herlea\\Pack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hearlea\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Herlea\\Pack\\PackServiceProvider"
            ]
        }
    }
}
